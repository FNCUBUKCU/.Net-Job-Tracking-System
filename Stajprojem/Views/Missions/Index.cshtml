@model IEnumerable<Stajprojem.Models.Missions>


<h2>Missions</h2>

<table class="table">
    <tr>
        <th style="color:Highlight">
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
